object BasicBranching

	def less(a: Int(32), b: Int(32)): Boolean :=
		a < b
	end less

	def lessOrEqual(a: Int(32), b: Int(32)): Boolean :=
		a <= b
	end lessOrEqual

	def test1(): Unit :=
		val a: Int(32) = 1;
		val b: Int(32) = a + 1;
		if(less(a, b)) then
			Std.printString("correct")
		else
			Std.printString("not correct")
		end if
	end test1

	def test2(): Unit :=
		val a: Int(32) = 1;
		val b: Int(32) = a + 1;
		if(less(a, 1)) then
			Std.printString("correct")
		else
			Std.printString("not correct")
		end if
	end test2

	def test3(): Unit :=
		val a: Int(32) = 1;
		val b: Int(32) = a + 1;
		if(less(b, a)) then
			Std.printString("correct")
		else
			Std.printString("not correct")
		end if
	end test3

	def test4(): Unit :=
		val a: Int(32) = 1;
		val b: Int(32) = a + 1;		
		if(lessOrEqual(a, a)) then
			Std.printString("correct")
		else
			Std.printString("not correct")
		end if
	end test4

	def test(): Unit :=
		val a: Int(32) = 1;
		val b: Int(32) = a + 1;
		if(less(a, b)) then
			Std.printString("correct")
		else
			Std.printString("not correct")
		end if;

		if(less(a, 1)) then
			Std.printString("correct")
		else
			Std.printString("not correct")
		end if;

		if(less(b, a)) then
			Std.printString("correct")
		else
			Std.printString("not correct")
		end if;

		if(lessOrEqual(a, a)) then
			Std.printString("correct")
		else
			Std.printString("not correct")
		end if
	end test

	test();

	Std.printString("test finished")
end BasicBranching
